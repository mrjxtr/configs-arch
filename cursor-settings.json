{
  // Update Settings
  "update.mode": "manual",
  "security.workspace.trust.untrustedFiles": "open",

  // Editor Settings
  "editor.defaultFormatter": "charliermarsh.ruff",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[django-html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.triggerExpansionOnTab": true,
  "ruff.lineLength": 80,
  "ruff.lint.enable": false,
  "prettier.tabWidth": 2,
  "editor.accessibilitySupport": "off",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.fontFamily": "JetBrains Mono, monospace",
  "editor.fontLigatures": true,
  "editor.fontVariations": true,
  "editor.fontWeight": "normal",
  "editor.fontSize": 15,
  "editor.letterSpacing": 1.5,
  "editor.lineHeight": 1.75,
  "editor.lineNumbers": "relative",
  "editor.lightbulb.enabled": "off",
  "editor.minimap.enabled": false,
  "editor.scrollbar.horizontalScrollbarSize": 10,
  "editor.scrollbar.verticalScrollbarSize": 10,
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": false,

  // Files Settings
  "files.autoSave": "afterDelay",
  "files.insertFinalNewline": true,
  "files.eol": "\n",
  "files.associations": {
    "*.html": "html"
  },

  // Material Icon Theme Settings
  "material-icon-theme.folders.associations": {
    "references": "meta",
    "reports": "enum",
    "venv": "environment"
  },
  "material-icon-theme.folders.color": "#90a4ae",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.saturation": 1,
  "material-icon-theme.folders.customClones": [
    {
      "name": "ds-data",
      "base": "database",
      "color": "yellow-800",
      "folderNames": ["data"]
    },
    {
      "name": "ds-data-raw",
      "base": "database",
      "color": "yellow-700",
      "folderNames": ["raw"]
    },
    {
      "name": "ds-data-external",
      "base": "database",
      "color": "yellow-700",
      "folderNames": ["external"]
    },
    {
      "name": "ds-data-interim",
      "base": "database",
      "color": "yellow-700",
      "folderNames": ["interim"]
    },
    {
      "name": "ds-data-processed",
      "base": "database",
      "color": "yellow-700",
      "folderNames": ["processed"]
    },
    {
      "name": "dashboards",
      "base": "review",
      "color": "teal-400",
      "folderNames": ["dashboards"]
    },
    {
      "name": "modeling",
      "base": "enum",
      "color": "red-400",
      "folderNames": ["modeling"]
    },
    {
      "name": "references",
      "base": "meta",
      "color": "blue-500",
      "folderNames": ["references"]
    },
    {
      "name": "cfg-utility",
      "base": "utils",
      "color": "green-600",
      "folderNames": ["utility"]
    },
    {
      "name": "scraping",
      "base": "debug",
      "color": "red-800",
      "folderNames": ["scraping"]
    }
  ],
  "material-icon-theme.files.associations": {
    "*.joblib": "sml"
  },

  // Window Settings
  "window.zoomLevel": 0.3,
  "search.quickOpen.includeHistory": false,
  "breadcrumbs.enabled": true,
  "window.newWindowProfile": "Default",
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "auto",
  "window.menuBarVisibility": "compact",
  "window.newWindowDimensions": "inherit",
  "window.titleBarStyle": "custom",
  "window.titleSeparator": " | ",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}${separator}${profileName}${separator}${appName}",

  // Explorer Settings
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,

  // Terminal Settings
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.windows": "pwsh",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.letterSpacing": 1.2,
  "terminal.integrated.inheritEnv": false,

  // Git Settings
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "git.autorefresh": true,

  // SCM Settings
  "scm.diffDecorations": "gutter",
  "scm.diffDecorationsGutterVisibility": "hover",
  "scm.diffDecorationsGutterWidth": 2,

  // Python Settings
  "python.defaultInterpreterPath": "/home/mrjxtr/miniconda3/bin/python",
  "python.condaPath": "/home/mrjxtr/miniconda3/condabin/conda",
  "python.analysis.inlayHints.callArgumentNames": "all",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.typeCheckingMode": "off",

  // Jupyter Settings
  "jupyter.askForKernelRestart": false,
  "jupyter.interactiveWindow.textEditor.executeSelection": true,

  // Notebook Settings
  "notebook.defaultFormatter": "charliermarsh.ruff",
  "notebook.formatOnCellExecution": true,
  "notebook.formatOnSave.enabled": true,
  "notebook.output.scrolling": true,
  "notebook.output.textLineLimit": 30,

  // Interactive Window Settings
  "interactiveWindow.collapseCellInputCode": "never",

