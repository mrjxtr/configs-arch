  "workbench.editorAssociations": {
    "file:/**/*.csv": "jupyter-data-wrangler",
    "file:/**/*.xlsx": "jupyter-data-wrangler",
    "*.csv": "default"
  },
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.showTabs": "multiple",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,

  // Custom Syntax Highlighting
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "meta.function-call.generic.python",
        "settings": {
          "foreground": "#8FCECA"
        }
      },
      {
        "scope": "meta.function-call.arguments.python",
        "settings": {
          "foreground": "#7FA2EE"
        }
      },
      {
        "scope": "keyword.control",
        "settings": {
          "foreground": "#BC8bDD"
        }
      },
      {
        "scope": "keyword.operator.assignment",
        "settings": {
          "foreground": "#BC8bDD"
        }
      }
    ]
  },

  // Extensions Settings
  "extensions.autoUpdate": false,
  "extensions.experimental.deferredStartupFinishedActivation": true,

  // Cursor IDE settings
  "workbench.activityBar.orientation": "vertical", // this is to fix the vim ctrl+l/h bug
  "cursor.chat.showPremiumChatHistory": false,
  "cursor.chat.collapseInputBoxPills": true,
  "cursor.chat.smoothStreaming": true,
  "cursor.composer.collapsePaneInputBoxPills": true,
  "cursor.chat.terminalShowHoverHint": false,
  "cursor.chat.showSuggestedFiles": true,

  // AutoDocstring Settings
  "autoDocstring.includeExtendedSummary": true,
  "autoDocstring.includeName": false,

  // SQLTools Settings
  "sqltools.connections": [],
  "sqltools.useNodeRuntime": false,

  // Markdown Preview Enhanced Settings
  "markdown-preview-enhanced.codeBlockTheme": "github-dark.css",
  "markdown-preview-enhanced.previewTheme": "github-dark.css",

  // CodeSnap Settings
  "codesnap.backgroundColor": "#aaaaaa",
  "codesnap.containerPadding": "2em",
  "codesnap.showWindowControls": false,

  // Cursor Settings
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorStyleInactive": "line",
  "terminal.integrated.cursorWidth": 3,
  "editor.cursorBlinking": "phase",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 3,
  "editor.cursorSurroundingLines": 8,
  "editor.cursorSmoothCaretAnimation": "on",

  //Vim Configurations
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.mouseSelectionGoesIntoVisualMode": true,
  "vim.useCtrlKeys": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    // Split Panes
    {
      "before": ["leader", "v"],
      "commands": [":vsplit"]
    },
    {
      "before": ["leader", "s"],
      "commands": [":split"]
    },
    // Save and Quit
    {
      "before": ["leader", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["leader", "q"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["leader", "k", "f"],
      "commands": ["workbench.action.closeFolder"]
    },
    // File Explorer
    {
      "before": ["leader", "e"],
      "commands": ["workbench.view.explorer"]
    },
    // Find Files
    {
      "before": ["leader", "space"],
      "commands": ["workbench.action.quickOpen"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    }
  ],
  "vim.handleKeys": {
    "<C-b>": false,
    "<C-e>": false,
    "<C-f>": false,
    "<C-j>": false,
    "<C-k>": false,
    "<C-p>": false,
    "<C-m>": false,
    "<C-i>": false
  },
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },

